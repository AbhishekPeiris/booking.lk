	</servlet>
	<servlet-mapping>
		<servlet-name>updatecustomer</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>editbooking</servlet-name>
		<servlet-class>com.admin.editBookingServelet</servlet-class>
	</servlet>
